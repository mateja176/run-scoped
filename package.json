    "postpublish": "cat package.json | json -e 'delete this.main' > package,json'"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^11.13.8",
    "lerna": "^3.4.3",
    "tsconfig-paths": "^3.8.0"
  }
}
